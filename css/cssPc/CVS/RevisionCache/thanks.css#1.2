#thanksContainer{
    
}

#thanksContent{
    width: 100%;
    height: 800px;
    font-size: 1.4em;
}

    #thanksContent tr{
        vertical-align: middle;
        text-align: center;        
    }
    
    #thanksContent td{
        width: 50%;
        background-color: #F9F9F9;
        border: 5px solid white;
    }
    
#thanksContent button{
    margin: 75px;
    cursor: pointer;
    padding: 0;
    width: 235px;
    height: 50px;
    border: none;
    color: white;
    text-align: center;
    font-size: 1.6em;
    -webkit-border-top-left-radius: 25px 25px;
    -moz-border-radius-topleft: 25px 25px;
    border-top-left-radius: 25px 25px;
    -webkit-border-bottom-left-radius: 25px 25px;
    -moz-border-radius-bottomleft: 25px 25px;
    border-bottom-left-radius: 25px 25px;
    -webkit-border-top-right-radius: 25px 25px;
    -moz-border-radius-topright: 25px 25px;
    border-top-right-radius: 25px 25px;
    -webkit-border-bottom-right-radius: 25px 25px;
    -moz-border-radius-bottomright: 25px 25px;
    border-bottom-right-radius: 25px 25px;
    background-color: #4C70A7;
}

#feedbackFormTable td{
    border: none; 
    background-color: white;
}

#backToChecklistButton{
    float: right;
    cursor: pointer;
    padding: 0;
    width: 100px;
    height: 25px;
    border: none;
    color: white;
    text-align: center;
    font-size: 1em;
    -webkit-border-top-left-radius: 25px 25px;
    -moz-border-radius-topleft: 25px 25px;
    border-top-left-radius: 25px 25px;
    -webkit-border-bottom-left-radius: 25px 25px;
    -moz-border-radius-bottomleft: 25px 25px;
    border-bottom-left-radius: 25px 25px;
    -webkit-border-top-right-radius: 25px 25px;
    -moz-border-radius-topright: 25px 25px;
    border-top-right-radius: 25px 25px;
    -webkit-border-bottom-right-radius: 25px 25px;
    -moz-border-radius-bottomright: 25px 25px;
    border-bottom-right-radius: 25px 25px;
    background-color: #4C70A7;
}

